package Address;

public class dog {
	String color;
	int age;
	String name;
	
	public dog(String name,String color,int age){
		this.name=name;
		this.color=color;
		this.age=age;
	}
	public void displaydog() {
		String m;
		m=name;
		m+=color;
		m+=age;
		System.out.println(m);
	}

}



package Address;

public class displaydog {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		dog D1=new dog("旺财","黄色",3);
		
		D1.displaydog();

	}

}
